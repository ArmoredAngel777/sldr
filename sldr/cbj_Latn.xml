<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml>
	<identity>
		<!--  Enter Language Name and ISO-639-3 code -->
		<language code="cbj" name="Ede Cabe"/>
		<!--  Enter Script Name and ISO-15924 Code  -->
		<script code="Latn" name="Latin"/>
	</identity>
	<!--  Enter the orientation attributes  -->
	<orientation>
		<orientation lines="left-to-right" characters="top-to-bottom"/>
	</orientation>
	<characters>
		<!--  Enter Auxiliary Exemplar List  -->
		<exemplarCharacters type="auxiliary">[c p q v x z]</exemplarCharacters>
		<!--  Enter Index Exemplar List  -->
		<exemplarCharacters type="index">[ A B D E Ɛ F G {Gb} H I J K {Kp} L M N O Ɔ R S {Sh} T U W Y]</exemplarCharacters>
		<!--  Enter Main Exemplar List  -->
		<!--  I guess we need to know which characters the accents go over and add them -->
		<exemplarCharacters>[a b d e ɛ f g {gb} h i j k {kp} l m n o ɔ r s {sh} t u w y \u0301 \u0300]</exemplarCharacters>
	</characters>
	<!--  Enter the status of the data from the following list of values:
            HOLD                 - Data should not be uploaded to ScriptSource
            Draft                - Data may be uploaded to ScriptSource  -->
	<!--  listed as HOLD because I'm not sure which characters the combining marks go with -->
	<status value="HOLD"/>
	<regionInfo>
		<!--  Enter the default region for this writing system (descriptive text)  -->
		<region type="Benin">region name</region>
		<!--  Enter the names of other countries where this writing system is used (descriptive text)  -->
		<region type="none">list of countries or regions</region>
	</regionInfo>
</ldml>
