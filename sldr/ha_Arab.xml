<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml>
	<identity>
		<!--  Enter Language Name and ISO-639-3 code -->
		<language code="hau" name="Hausa"/>
		<!--  Enter Script Name and ISO-15924 Code  -->
		<script code="Arab" name="Arabic"/>
	</identity>
	<!--  Enter the orientation attributes  -->
	<orientation>
		<characterOrder>right-to-left</characterOrder>
		<lineOrder>top-to-bottom</lineOrder>
	</orientation>
	<characters>
		<exemplarCharacters type="auxiliary">[\u200C\u200D\u200E\u200F]</exemplarCharacters>
		<exemplarCharacters type="digits">[٠ ١ ٢ ٣ ٤ ٥ ٦ ٧ ٨ ٩]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[! ( ) * - . / : \ [ ] ^ « » ؀ ، ؛ ؞ ؟ ‏ • ‰ ‹ › \u0640 \u06DD \uFD3E \uFD3F ]</exemplarCharacters>
		<!--  Enter Main Exemplar List  -->
		<exemplarCharacters>[\u0610 \u0611 \u0612 \u0613 \u0614 \u064B \u064E \u064F \u0650 \u0651 \u0652 \u0654 \u0657 \u065C \u065D \u0670 \u08F9 ء آ أ إ ئ ا ب ة ت ث ج ح خ د ذ ر ز س ش ص ض ط ظ ع غ ف ق ك ل م ن ه و ى ي ٯ ٻ پ ڃ چ ڋ ژ ڟ ڡ ڢ ڤ ڧ ک ڪ گ ں ۈ ۉ ی ې ۑ ے ݖ ݧ]</exemplarCharacters>
	</characters>
	<!--  Enter the status of the data from the following list of values:
            HOLD                 - Data should not be uploaded to ScriptSource
            Draft                - Data may be uploaded to ScriptSource  -->
	<status value="Draft"/>
	<regionInfo>
		<!--  Enter the names of other countries where this writing system is used (descriptive text)  -->
		<region type="Niger">list of countries or regions</region>
		<!--  Enter the default region for this writing system (descriptive text)  -->
		<region type="Nigeria">region name</region>
	</regionInfo>
</ldml>
