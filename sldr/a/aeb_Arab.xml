			</sil:kbd>
    <sil:spell-checking type="hunspell">
	<sil:url>https://wirl.api.sil.org/aeb-DictList</sil:url>
    </sil:spell-checking>

		</sil:external-resources>
	</special>
	<layout>
		<!--  Enter the orientation attributes  -->
		<orientation>
			<characterOrder>right-to-left</characterOrder>
		</orientation>
