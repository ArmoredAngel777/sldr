<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml>
	<identity>
		<!--  Enter Language Name and ISO-639-3 code -->
		<language code="khw" name="Khowar"/>
		<!--  Enter Script Name and ISO-15924 Code  -->
		<script code="Arab" name="Arabic"/>
	</identity>
	<!--  Enter the orientation attributes  -->
	<orientation>
		<characterOrder>right-to-left</characterOrder>
		<lineOrder>top-to-bottom</lineOrder>
	</orientation>
	<characters>
		<exemplarCharacters type="auxiliary">[\u200C\u200D\u200E\u200F]</exemplarCharacters>
		<!--  Enter Main Exemplar List  -->
		<exemplarCharacters>[ا ب پ ت ٹ ث ج ݮ چ څ ح خ د ڈ ذ ر ڑ ز ژ ݱ س ش ݰ ص ض ط ظ ع غ ف ق ک گ ل م ن و ہ ء ی ے]</exemplarCharacters>
	</characters>
	<!--  Enter the status of the data from the following list of values:
            HOLD                 - Data should not be uploaded to ScriptSource
            Draft                - Data may be uploaded to ScriptSource  -->
	<status value="Draft"/>
</ldml>
