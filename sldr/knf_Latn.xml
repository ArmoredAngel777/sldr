<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml>
	<identity>
		<generation date="$Date$"/>
		<!--  Enter Language Name and ISO-639-3 code -->
		<language code="knf" name="Mankanya"/>
		<!--  Enter Script Name and ISO-15924 Code  -->
		<script code="Latn" name="Latin"/>
	</identity>
	<!--  Enter the orientation attributes  -->
	<orientation>
		<characterOrder>left-to-right</characterOrder>
		<lineOrder>top-to-bottom</lineOrder>
	</orientation>
	<characters>
		<!--  Enter Auxiliary Exemplar List  -->
		<exemplarCharacters type="auxiliary">[q s v x z]</exemplarCharacters>
		<!--  Enter Index Exemplar List  -->
		<exemplarCharacters type="index">[A B C D E Ë F G H I J K L M N Ñ Ŋ O P R Ş T Ţ Ŧ U Ú W Y]</exemplarCharacters>
		<!--  Enter Punctuation Exemplar List  -->
		<exemplarCharacters type="punctuation">[. ? ! , : ; « » " “ ” ' ‘ ’ -]</exemplarCharacters>
		<!--  Enter Main Exemplar List  -->
		<exemplarCharacters>[a b c d e ë f g h i j k l m n {ñ} ŋ o p r ş t ţ ŧ u ú w y]</exemplarCharacters>
	</characters>
	<!--  Enter the plural rules (template example is for only one plural form)  -->
	<plurals>
		<pluralRule count="one">n is 1</pluralRule>
	</plurals>
	<!--  Enter the status of the data from the following list of values:
 	  		Draft 				 - Data just received, possibly more data to come
			Pending Verification - All data received, confirmation requested from user that data is correct
			Verified			 - User has confirmed that data is correct
			Approved			 - Approved by ScriptSource team as being ready for submission to the CLDR and released to
									   ScriptSource and other applications
			Submitted to CLDR	 - Data added to the CLDR SVN Repository for their vetting process to begin
			Released			 - Data now available from the CLDR repository, not to be picked up from CLDS  -->
	<status value="Approved"/>
	<regionInfo>
		<!--  Enter the default region for this writing system (descriptive text)  -->
		<region type="default">Guinea-Bissau</region>
		<!--  Enter the names of other countries where this writing system is used (descriptive text)  -->
		<region type="other">Senegal, The Gambia</region>
	</regionInfo>
</ldml>
